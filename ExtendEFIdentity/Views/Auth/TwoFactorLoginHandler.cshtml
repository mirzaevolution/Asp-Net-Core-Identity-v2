@model TwoFactorLoginHandlerViewModel
@{
	ViewData["Title"] = "Two Factor Login";
}

<h4 class="text-primary">Select Two Factor Login Provider(s)</h4>
<div class="row">
	<div class="col-md-5">
		<form method="post">
			@Html.AntiForgeryToken()
			<input type="hidden" name="Email" value="@Model.Email" />
			<div class="form-group">
				<select name="Provider" class="form-control" asp-items="new SelectList(@Model.Providers)"></select>
			</div>
			<div class="form-group">
				<button type="submit" class="btn btn-primary">Submit</button>
			</div>
		</form>


	</div>
</div>